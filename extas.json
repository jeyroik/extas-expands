{
  "name": "extas/protocol-expand",
  "installer_version": 3,
  "package_classes": [
    {
      "interface": "expandRequiredRepository",
      "class": "extas\\components\\expands\\ExpandRequiredRepository"
    }
  ],
  "commands_options": [
    {
      "name": "only-expand",
      "shortcut": "",
      "mode": 4,
      "title": "",
      "description": "",
      "default": "",
      "commands": ["extas-install"],
      "class": "extas\\"
    }
  ],
  "plugins": [
    {
      "class": "extas\\components\\plugins\\init\\InitExpandsRequired",
      "stage": "extas.init.section.required_expands",
      "install_on": "initialization"
    },
    {
      "class": "extas\\components\\plugins\\servers\\PluginServerDispatchRouteBefore",
      "stage": "route.before"
    }
  ],
  "protocols": [
    {
      "name": "expand",
      "title": "Extas expand",
      "description": "Parse headers and request parameters for expand attribute",
      "accept": ["*"],
      "class": "extas\\components\\protocols\\ProtocolExpand"
    }
  ],
  "extensions": [
    {
      "class": "extas\\components\\extensions\\ExtensionRepositoryGet",
      "interface": "extas\\interfaces\\extensions\\IExtensionRepositoryGet",
      "subject": "*",
      "methods": ["expandRequiredRepository"],
      "install_on": "initialization"
    }
  ]
}